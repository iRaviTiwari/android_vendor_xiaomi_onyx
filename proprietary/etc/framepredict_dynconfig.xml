<?xml version="1.0" encoding="UTF-8"?>
<dynamic_frame_rescue_config>
    <support_mode>
        <mode name="performance">performance</mode>
        <mode name="balanced">balanced</mode>
    </support_mode>
    <thermal>
        <trig temp="25000" big="2600" pri="3000" glv="0">25</trig>
        <trig temp="30000" big="2500" pri="2900" glv="1">30</trig>
        <trig temp="35000" big="2200" pri="2600" glv="2">35</trig>
        <trig temp="40000" big="2000" pri="2200" glv="5">40</trig>
        <trig temp="99000" big="1600" pri="1800" glv="8">99</trig>
    </thermal>
    <default>
        <power_mode mode="performance">
            <cduration lv1="30" lv2="50" lv3="100">cduration</cduration>
            <cthreshold lv1="0.2" lv2="0.6" lv3="1.1">cthreshold</cthreshold>
            <freqbig lv1="2000" lv2="2400" lv3="2800">freqbig</freqbig>
            <freqpri lv1="2200" lv2="2600" lv3="3000">freqpri</freqpri>
            <gduration lv1="30" lv2="50" lv3="100">gduration</gduration>
            <gthreshold lv1="0.3" lv2="0.8">gthreshold</gthreshold>
            <glevel lv1="0" lv2="0" lv3="0">glevel</glevel>
        </power_mode>
        <power_mode mode="balanced">
            <cduration lv1="30" lv2="50" lv3="100">cduration</cduration>
            <cthreshold lv1="0.3" lv2="0.7" lv3="1.2">cthreshold</cthreshold>
            <freqbig lv1="1800" lv2="2200" lv3="2600">freqbig</freqbig>
            <freqpri lv1="2000" lv2="2400" lv3="2800">freqpri</freqpri>
            <gduration lv1="30" lv2="50" lv3="100">gduration</gduration>
            <gthreshold lv1="0.4" lv2="0.9">gthreshold</gthreshold>
            <glevel lv1="1" lv2="1" lv3="0">glevel</glevel>
        </power_mode>
        <power_mode mode="power">
            <cduration lv1="30" lv2="50" lv3="100">cduration</cduration>
            <cthreshold lv1="0.4" lv2="0.8" lv3="1.3">cthreshold</cthreshold>
            <freqbig lv1="1600" lv2="2000" lv3="2400">freqbig</freqbig>
            <freqpri lv1="1800" lv2="2200" lv3="2600">freqpri</freqpri>
            <gduration lv1="30" lv2="50" lv3="100">gduration</gduration>
            <gthreshold lv1="0.5" lv2="1.0">gthreshold</gthreshold>
            <glevel lv1="5" lv2="5" lv3="3">glevel</glevel>
        </power_mode>
        <power_mode mode="performance_out">
            <cduration lv1="30" lv2="50" lv3="100">cduration</cduration>
            <cthreshold lv1="0.2" lv2="0.6" lv3="1.1">cthreshold</cthreshold>
            <freqbig lv1="1900" lv2="2300" lv3="2700">freqbig</freqbig>
            <freqpri lv1="2100" lv2="2500" lv3="2900">freqpri</freqpri>
            <gduration lv1="30" lv2="50" lv3="100">gduration</gduration>
            <gthreshold lv1="0.3" lv2="0.8">gthreshold</gthreshold>
            <glevel lv1="1" lv2="1" lv3="1">glevel</glevel>
        </power_mode>
        <power_mode mode="balanced_out">
            <cduration lv1="30" lv2="50" lv3="100">cduration</cduration>
            <cthreshold lv1="0.3" lv2="0.7" lv3="1.2">cthreshold</cthreshold>
            <freqbig lv1="1700" lv2="2100" lv3="2500">freqbig</freqbig>
            <freqpri lv1="1900" lv2="2300" lv3="2700">freqpri</freqpri>
            <gduration lv1="30" lv2="50" lv3="100">gduration</gduration>
            <gthreshold lv1="0.4" lv2="0.9">gthreshold</gthreshold>
            <glevel lv1="2" lv2="2" lv3="1">glevel</glevel>
        </power_mode>
        <power_mode mode="power_out">
            <cduration lv1="30" lv2="50" lv3="100">cduration</cduration>
            <cthreshold lv1="0.4" lv2="0.8" lv3="1.3">cthreshold</cthreshold>
            <freqbig lv1="1500" lv2="1900" lv3="2300">freqbig</freqbig>
            <freqpri lv1="1700" lv2="2100" lv3="2500">freqpri</freqpri>
            <gduration lv1="30" lv2="50" lv3="100">gduration</gduration>
            <gthreshold lv1="0.5" lv2="1.0">gthreshold</gthreshold>
            <glevel lv1="6" lv2="6" lv3="4">glevel</glevel>
        </power_mode>
