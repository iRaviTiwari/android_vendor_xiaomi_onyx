service vendor.cld-hal-aidl /vendor/bin/hw/vendor.xiaomi.hardware.cld_aidl-service
    class hal
    user root
    group root

on property:persist.sys.stability.smartgc.ctrl=*
    setprop vendor.smartgc.ctrl ${persist.sys.stability.smartgc.ctrl}

on property:persist.sys.stability.smartgc.count=*
    setprop vendor.smartgc.count ${persist.sys.stability.smartgc.count}
